{
  "name": "@roleup/redis",
  "description": "Redis wrapper",
  "version": "3.0.0",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/roleup/redis/issues"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/bluebird-global": "^3.5.12",
    "@types/fs-extra": "^8.0.1",
    "@types/ioredis": "^4.0.18",
    "@types/lodash": "^4.14.144",
    "@types/luxon": "^1.15.2",
    "@types/node": "^12.12.5",
    "@types/redlock": "^4.0.0",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.15",
    "redlock": "^4.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@roleup/eslint-config": "^2.0.5",
    "@roleup/prettier-config": "^1.0.1",
    "@types/chai-as-promised": "^7.1.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "husky": "^3.1.0",
    "ioredis": "^4.14.1",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "ts-node": "^8.4.1",
    "ts-sinon": "^1.0.24",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.2",
    "typings": "^2.1.1",
    "uuid": "^3.3.3"
  },
  "files": [
    "*.ts",
    "dist/*"
  ],
  "homepage": "https://github.com/roleup/redis#readme",
  "husky": {
    "hooks": {
      "pre-commit": "LIST=`git diff-index --cached --name-only HEAD | grep '.*\\.js' | grep -v json`; if [ \"$LIST\" ]; then npm run lint && git add $LIST; fi"
    }
  },
  "keywords": [
    "client",
    "redis"
  ],
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "fixpack",
      "git add"
    ],
    "**/*.js, **/*.ts": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "peerDependencies": {
    "ioredis": "^4.5.1"
  },
  "prettier": "@roleup/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roleup/redis.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "integration": "nyc mocha -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.it.js\"",
    "lint": "fixpack && prettier-eslint --write && eslint --fix \"**/*.{js,ts}\"",
    "prepare": "fixpack",
    "test": "mocha \"tests/**.unit.js\" && npm run lint",
    "test-most": "mocha \"tests/**.unit.js\" \"tests/**.it.js\" --exit && npm run lint",
    "unit": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/lib/**/*.unit.+(ts|js)\""
  },
  "types": "dist/index.d.ts"
}
